<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_reservation_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyReservationScreen">

    <View
        android:id="@+id/rec_background"
        android:layout_width="match_parent"
        android:layout_height="118dp"
        android:background="#1B262C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/search_rec"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/rectangle_myreservationscr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sugbo_eats" />

    <TextView
        android:id="@+id/sugbo_eats"
        android:layout_width="105dp"
        android:layout_height="19dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:gravity="center_horizontal|top"
        android:text="@string/all_cap"
        android:textAppearance="@style/sugbo_eats"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/search"
        android:layout_width="40dp"
        android:layout_height="39dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/search"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sugbo_eats" />

    <View
        android:id="@+id/bottom_bar"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/bottom_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rec_background"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imagehome"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="3dp"
        android:background="@null"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="@id/bottom_bar"
        app:layout_constraintEnd_toEndOf="@id/bottom_bar"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="@id/bottom_bar"
        app:layout_constraintTop_toTopOf="@+id/bottom_bar"
        app:layout_constraintVertical_bias="0.617" />

    <ImageButton
        android:id="@+id/imagesched"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="5dp"
        android:background="@null"
        android:src="@drawable/schedule"
        app:layout_constraintBottom_toBottomOf="@id/bottom_bar"
        app:layout_constraintEnd_toEndOf="@id/bottom_bar"
        app:layout_constraintHorizontal_bias="0.385"
        app:layout_constraintStart_toStartOf="@id/bottom_bar"
        app:layout_constraintTop_toTopOf="@+id/bottom_bar"
        app:layout_constraintVertical_bias="0.656" />

    <ImageButton
        android:id="@+id/imagefave"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="5dp"
        android:background="@null"
        android:src="@drawable/favicon"
        app:layout_constraintBottom_toBottomOf="@id/bottom_bar"
        app:layout_constraintEnd_toEndOf="@id/bottom_bar"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintStart_toStartOf="@id/bottom_bar"
        app:layout_constraintTop_toTopOf="@+id/bottom_bar"
        app:layout_constraintVertical_bias="0.656" />

    <ImageButton
        android:id="@+id/imageprof"
        android:layout_width="32dp"
        android:layout_height="35dp"
        android:layout_marginBottom="5dp"
        android:background="@null"
        android:src="@drawable/proficon"
        app:layout_constraintBottom_toBottomOf="@id/bottom_bar"
        app:layout_constraintEnd_toEndOf="@id/bottom_bar"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintStart_toStartOf="@id/bottom_bar"
        app:layout_constraintTop_toTopOf="@+id/bottom_bar"
        app:layout_constraintVertical_bias="0.656" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/notifications_unread"
        app:layout_constraintBottom_toBottomOf="@+id/rec_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.17"
        app:srcCompat="@drawable/notifications_unread" />

    <TextView
        android:id="@+id/textView11"
        style="@style/my_reservat"
        android:layout_width="175dp"
        android:layout_height="29dp"
        android:text="@string/reservation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.169" />

    <View
        android:id="@+id/reserve_view"
        android:layout_width="360dp"
        android:layout_height="138dp"
        android:background="@drawable/reserve_viewer"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.249" />

    <TextView
        android:id="@+id/resto_name"
        style="@style/resto_name"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:layout_marginTop="13dp"
        android:text="Frandel and Spies"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toTopOf="@+id/reserve_view" />

    <TextView
        android:id="@+id/date_and_time"
        style="@style/date_and_time"
        android:layout_width="160dp"
        android:layout_height="16dp"
        android:layout_marginTop="2dp"
        android:text="06/02/2024 - 7:00 pm"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toBottomOf="@+id/resto_name" />

    <TextView
        android:id="@+id/resto_loc"
        style="@style/resto_loc"
        android:layout_width="160dp"
        android:layout_height="18dp"
        android:layout_marginTop="2dp"
        android:text="MJ Cuenco, Cor, Palmabakak hefjah f"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toBottomOf="@+id/date_and_time" />

    <TextView
        android:id="@+id/setting_type"
        style="@style/set_type"
        android:layout_width="160dp"
        android:layout_height="18dp"
        android:layout_marginTop="2dp"
        android:text="Setting Type: Alfresco"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toBottomOf="@+id/resto_loc" />

    <TextView
        android:id="@+id/num_guest"
        style="@style/num_guest"
        android:layout_width="160dp"
        android:layout_height="18dp"
        android:layout_marginTop="1dp"
        android:text="Number of Guests: 8"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toBottomOf="@+id/setting_type" />

    <TextView
        android:id="@+id/textView27"
        style="@style/decision"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel Reservation"
        app:layout_constraintBottom_toBottomOf="@+id/reserve_view"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toTopOf="@+id/reserve_view"
        app:layout_constraintVertical_bias="0.942" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="111dp"
        android:layout_height="94dp"
        app:layout_constraintBottom_toBottomOf="@+id/reserve_view"
        app:layout_constraintEnd_toEndOf="@+id/reserve_view"
        app:layout_constraintHorizontal_bias="0.911"
        app:layout_constraintStart_toStartOf="@+id/reserve_view"
        app:layout_constraintTop_toTopOf="@+id/reserve_view"
        app:layout_constraintVertical_bias="0.295"
        app:srcCompat="@drawable/frndl" />


</androidx.constraintlayout.widget.ConstraintLayout>